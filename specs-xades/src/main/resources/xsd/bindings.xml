<jaxb:bindings version="3.0"
	xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<jaxb:bindings schemaLocation="XAdESv141.xsd" node="//xs:schema">
		<jaxb:schemaBindings>
			<jaxb:package name="eu.europa.esig.xades.jaxb.xades141" />
		</jaxb:schemaBindings>
		<jaxb:bindings
			schemaLocation="http://uri.etsi.org/01903/v1.3.2/XAdES.xsd"	node="//xs:schema">
			<jaxb:schemaBindings>
				<jaxb:package name="eu.europa.esig.xades.jaxb.xades132" />
			</jaxb:schemaBindings>
		</jaxb:bindings>
	
		<jaxb:bindings schemaLocation="XAdESv122.xsd" node="//xs:schema">
			<jaxb:schemaBindings>
				<jaxb:package name="eu.europa.esig.xades.jaxb.xades122" />
			</jaxb:schemaBindings>
		</jaxb:bindings>
		
		<jaxb:bindings schemaLocation="XAdESv111.xsd" node="//xs:schema">
			<jaxb:schemaBindings>
				<jaxb:package name="eu.europa.esig.xades.jaxb.xades111" />
			</jaxb:schemaBindings>
		</jaxb:bindings>
	</jaxb:bindings>
	
    <jaxb:bindings schemaLocation="http://uri.etsi.org/01903/v1.3.2/XAdES.xsd" node="//xs:simpleType[@name='QualifierType']">
		<jaxb:javaType name="eu.europa.esig.dss.enumerations.ObjectIdentifierQualifier" 
			parseMethod="eu.europa.esig.dss.jaxb.parsers.ObjectIdentifierQualifierParser.parse" 
			printMethod="eu.europa.esig.dss.jaxb.parsers.ObjectIdentifierQualifierParser.print" />
	</jaxb:bindings>
	
    <jaxb:bindings schemaLocation="XAdESv122.xsd" node="//xs:simpleType[@name='QualifierType']">
		<jaxb:javaType name="eu.europa.esig.dss.enumerations.ObjectIdentifierQualifier" 
			parseMethod="eu.europa.esig.dss.jaxb.parsers.ObjectIdentifierQualifierParser.parse" 
			printMethod="eu.europa.esig.dss.jaxb.parsers.ObjectIdentifierQualifierParser.print" />
	</jaxb:bindings>
	
    <jaxb:bindings schemaLocation="XAdESv111.xsd" node="//xs:simpleType[@name='QualifierType']">
		<jaxb:javaType name="eu.europa.esig.dss.enumerations.ObjectIdentifierQualifier" 
			parseMethod="eu.europa.esig.dss.jaxb.parsers.ObjectIdentifierQualifierParser.parse" 
			printMethod="eu.europa.esig.dss.jaxb.parsers.ObjectIdentifierQualifierParser.print" />
	</jaxb:bindings>

</jaxb:bindings>
